name = "Sum of all"
problem = "You gets several integers. Sum it."
example = [2, 3, 5, 9]
inputs = "%i+[0:10**10]"

# inputs: %i - int, %f - float, %s - string, %l - list of
#   * - 0 and more
#   + - 1 and more
#   ? - 0 or 1
#   {a} - a times
#   {a:b} - b >= times >= a
#   {a:} - times >= a
#   [a:] - x >= a
#   [:b] - x <= b
#   [a:b] - a <= x <= b
